@model STAMVCCRUD.Models.UpdateDosenViewModel

@{
}

<h1>Update Dosen</h1>
<form method="post" action="ViewDosen" class="mt-5">
    <div class="mb-3">
        <label for="" class="form-label">NIP</label>
        <input type="text" class="form-control" asp-for="Nip" readonly>
    </div>
    <div class="mb-3">
        <label for="" class="form-label">Nama Dosen</label>
        <input type="text" class="form-control" asp-for="Nama_Dosen">
    </div>

    <button type="submit" class="btn btn-primary">Update</button>

    <button type="submit" class="btn btn-danger" asp-action="Delete" asp-controller="Dosen">Delete</button>
</form>

