@model STAMVCCRUD.Models.AddMatakuliahViewModel;
@{
}


<h1>Tambah Matakuliah</h1>
<form method="post" action="AddMk" class="mt-5">
    <div class="mb-3">
        <label for="" class="form-label">Kode Matakuliah</label>
        <input type="text" class="form-control" asp-for="Kode_MK">
    </div>
    <div class="mb-3">
        <label for="" class="form-label">Nama Matakuliah</label>
        <input type="text" class="form-control" asp-for="Nama_MK">
    </div>
    <div class="mb-3">
        <label for="" class="form-label">SKS</label>
        <input type="number" class="form-control" asp-for="Sks">
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>