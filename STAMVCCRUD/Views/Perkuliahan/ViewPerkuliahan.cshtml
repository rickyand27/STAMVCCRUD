@model STAMVCCRUD.Models.UpdatePerkuliahanViewModel
@{
}

<h1>Update Perkuliahan</h1>
<form method="post" action="ViewPerkuliahan" class="mt-5">
    <div class="mb-3">
        <label for="" class="form-label">ID Perkuliahan</label>
        <input type="text" class="form-control" asp-for="IdPerkuliahan" readonly>
    </div>
    <div class="mb-3">
        <label for="" class="form-label">NIM</label>
        <input type="text" class="form-control" asp-for="Nim">
    </div>
    <div class="mb-3">
        <label for="" class="form-label">Kode Matakuliah</label>
        <input type="text" class="form-control" asp-for="Kode_MK">
    </div>
    <div class="mb-3">
        <label for="" class="form-label">NIP</label>
        <input type="text" class="form-control" asp-for="Nip">
    </div>
    <div class="mb-3">
        <label for="" class="form-label">Nilai</label>
        <input type="text" class="form-control" asp-for="Nilai">
    </div>
    

    <button type="submit" class="btn btn-primary">Update</button>

    <button type="submit" class="btn btn-danger" asp-action="Delete" asp-controller="Perkuliahan">Delete</button>
</form>